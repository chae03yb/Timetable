<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/css/timetable.css" rel="stylesheet">
    <title> 시간표 </title>
</head>
<body>
    <table id="timetable">
        <thead>
            <tr>
                <th>월</th>
                <th>화</th>
                <th>수</th>
                <th>목</th>
                <th>금</th>
            </tr>
        </thead>
        <tbody>
            {{#each Timetable}}
                <tr>
                    <td><button>{{Mon.ClassName}}</button></td>
                    <td><button>{{Tue.ClassName}}</button></td>
                    <td><button>{{Wed.ClassName}}</button></td>
                    <td><button>{{Thu.ClassName}}</button></td>
                    <td><button>{{Fri.ClassName}}</button></td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</body>