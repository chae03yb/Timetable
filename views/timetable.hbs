<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> 시간표 </title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>월</th>
                <th>화</th>
                <th>수</th>
                <th>목</th>
                <th>금</th>
            </tr>
        </thead>
        <tbody>
            {{#each Timetable}}
                <tr>
                    <td>{{Mon.classname}}</td>
                    <td>{{Tue.classname}}</td>
                    <td>{{Wed.classname}}</td>
                    <td>{{Thu.classname}}</td>
                    <td>{{Fri.classname}}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</body>